  "compilerOptions": {
    "target": "ES2018",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "isolatedModules": true
  }
}
